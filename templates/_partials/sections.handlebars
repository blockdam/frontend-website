
{{#each body.sections as |section|}}

    <section class="section {{section.type}} {{containerClass section.type}} {{section.style}}">

        {{#ifEquals section.type 'paragraph'}}
            <div class="text">{{{section.text}}}</div>
        {{/ifEquals}}

        {{#ifEquals section.type 'streamer'}}
            <div>
				{{{section.text}}}
            </div>
        {{/ifEquals}}

        {{#ifEquals section.type 'image-single-large'}}

        <div class="col-container">
            {{> responsive-image title=section.image.alttext external_id=section.image.external_id filename=section.image.filename }}
        </div>
        <div class="side-text side-text--small col-aside">
            <span class="text">{{ section.image.caption }}</span>
            {{#if section.image.credit}}<span class="photographer">Foto: {{section.image.credit}}</span>{{/if}}
        </div>

        {{/ifEquals}}

        {{#ifEquals section.type 'image-single-left'}}

          <div class="col-plus-aside">
              {{> responsive-image title=section.image.alttext external_id=section.image.external_id filename=section.image.filename }}
          </div>
          <div class="side-text side-text--small col-aside">
              <span class="text">{{ section.image.caption }}</span>
              {{#if section.image.credit}}<span class="photographer">Foto: {{section.image.credit}}</span>{{/if}}
          </div>

        {{/ifEquals}}

        {{#ifEquals section.type 'table-of-contents-heading'}}
			{{> floating-header text=section.text class="table-of-contents-header" }}
        {{/ifEquals}}

        {{#ifEquals section.type 'image-single-right'}}

            <div class="side-text side-text--small col-aside">
                <span class="text">{{ section.image.caption }}</span>
                {{#if section.image.credit}}<span class="photographer">Foto: {{section.image.credit}}</span>{{/if}}
            </div>
            <div class="col-plus-aside">
                {{> responsive-image title=section.image.alttext external_id=section.image.external_id filename=section.image.filename }}
            </div>

        {{/ifEquals}}

        {{#ifEquals section.type 'images-duo-full-width'}}

            <div class="cluster-left">
              {{> responsive-image title=section.image_left.alttext external_id=section.image_left.external_id filename=section.image_left.filename }}
                <!-- <div class="overlay">
                    <span class="multiline right-aligned"><span><span>{{section.image_left.caption}}</span></span></span>
                    <span class="credits smallerfont">Foto: {{section.image_left.credit}}</span>
                </div> -->
            </div>
            <div class="cluster-right">
                {{> responsive-image title=section.image_right.alttext external_id=section.image_right.external_id filename=section.image_right.filename }}
                <!-- <div class="overlay">
                    <span class="multiline right-aligned"><span><span>{{section.image_right.caption}}</span></span></span></span>
                    <span class="credits smallerfont">Foto: {{section.image_right.credit}}</span>
                </div> -->
            </div>

        {{/ifEquals}}

        {{#ifEquals section.type 'images-duo'}}

            <div class="col-half">
              {{> responsive-image title=section.image_left.alttext external_id=section.image_left.external_id filename=section.image_left.filename }}
